{
  "name": "lwc-dev-server",
  "version": "1.0.8",
  "description": "Local dev server",
  "main": "dist/server/LocalDevServer.js",
  "types": "dist/server/LocalDevServer.d.ts",
  "author": "Lightning Tooling",
  "license": "MIT",
  "repository": "forcedotcom/lwc-dev-server",
  "bin": {
    "lwc-dev-server": "./bin/run"
  },
  "oclif": {
    "macos": {
      "identifier": "com.salesforce.lwcdevserver"
    },
    "commands": "./dist/cli/commands",
    "bin": "lwc-dev-server",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-update"
    ],
    "update": {
      "s3": {
        "bucket": "bucketeer-42b78764-a91c-4b23-a7ba-4c047290ae18"
      }
    }
  },
  "files": [
    "bin",
    "dist",
    "!dist/__test__",
    "lib",
    "!lib/unpacked",
    "modules",
    "script",
    "vendors",
    "/messages",
    "/oclif.manifest.json"
  ],
  "lwc": {
    "mapNamespaceFromPath": true,
    "modules": [
      "modules"
    ]
  },
  "scripts": {
    "clean": "yarn rimraf dist reports coverage oclif.manifest.json",
    "build": "yarn clean && tsc && ts-node script/copy-assets",
    "watch": "tsc -w | awk '{gsub(/\\033c/,\"\") system(\"\")}1'",
    "lint": "yarn eslint src --ext js,ts",
    "lint:ci": "yarn lint --format junit --output-file ./reports/junit/eslint-results.xml --no-color",
    "start:todo": "node ./bin/run server test-projects/lwc-todomvc-e8217b7 todo/app",
    "start:todo:debug": "node --inspect-brk ./bin/run server test-projects/lwc-todomvc-e8217b7 todo/app",
    "test": "yarn jest --coverage",
    "test:watch": "yarn test --watch",
    "test:debug": "node --inspect node_modules/.bin/jest src --runInBand",
    "test:ci": "yarn test --ci",
    "test:e2e": "HEADLESS=false yarn jest --config integration-tests/jest.config.integration-test.js",
    "test:e2e-ci": "yarn jest --config integration-tests/jest.config.integration-test.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "link-talon": "yarn link @talon/common @talon/framework @talon/compiler",
    "unlink-talon": "yarn unlink @talon/common @talon/framework @talon/compiler",
    "precommit": "lint-staged",
    "prepush": "yarn prepare && yarn lint && yarn test",
    "preinstall": "script/libs-prepare.js",
    "postinstall": "script/libs-install.js && script/postinstall-build.js",
    "prepack": "oclif-dev manifest",
    "postpack": "rm -f oclif.manifest.json",
    "prepublishOnly": "yarn build && script/packagejson-clean.js",
    "postpublish": "script/packagejson-restore.js"
  },
  "devDependencies": {
    "@lwc/jest-preset": "^0.39.1-222.2",
    "@lwc/jest-resolver": "^0.39.1-222.2",
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@oclif/tslint": "^3",
    "@salesforce-ux/design-system": "^2.6.1",
    "@salesforce/lwc-jest": "^0.5.0",
    "@types/cpx": "^1.5.0",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.11",
    "@types/mock-fs": "^3.6.30",
    "@types/shelljs": "^0.8.4",
    "@types/xml2js": "^0.4.4",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "aws-sdk": "^2.445.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^2.1.0",
    "jest": "^24.7.1",
    "jest-junit": "^6.3.0",
    "jest-runner-eslint": "^0.7.3",
    "lint-staged": "^8.1.5",
    "mock-fs": "^4.8.0",
    "prettier": "^1.17.0",
    "rimraf": "^2.6.3",
    "selenium-standalone": "^6.16.0",
    "ts-jest": "^24.0.2",
    "ts-mockito": "^2.3.1",
    "ts-node": "^8.1.0",
    "tslint": "^5.0.0",
    "typescript": "^3.4.5",
    "webdriverio": "^5.9.1"
  },
  "dependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/runtime": "^7.4.4",
    "@lwc/compiler": "^0.39.1-222.2",
    "@lwc/engine": "^0.39.1-222.2",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/plugin-update": "^1.3.9",
    "@salesforce/command": "^1.5.0",
    "@salesforce/core": "^1.3.3",
    "@salesforce/ts-types": "^1.1.3",
    "@talon/common": "file:lib/unpacked/talon-common-0.11.10",
    "@talon/compiler": "file:lib/unpacked/talon-compiler-0.11.10",
    "@talon/force-modules": "file:lib/unpacked/talon-force-modules-0.0.5",
    "@talon/framework": "file:lib/unpacked/talon-framework-0.11.10",
    "@talon/metadata-schema": "file:lib/unpacked/talon-metadata-schema-0.0.11",
    "colors": "^1.3.3",
    "compression": "^1.7.4",
    "connect-livereload": "^0.6.1",
    "cookie-parser": "^1.4.4",
    "cpx": "^1.5.0",
    "csurf": "^1.10.0",
    "debug": "^4.1.1",
    "express": "^4.16.4",
    "fast-glob": "^2.2.6",
    "fs-extra": "^7.0.1",
    "gunzip-maybe": "^1.4.1",
    "helmet": "^3.16.0",
    "livereload": "^0.7.0",
    "lwc-components-lightning": "file:lib/unpacked/lwc-components-lightning-1.2.1-alpha",
    "mime-types": "^2.1.24",
    "path": "^0.12.7",
    "shelljs": "^0.8.3",
    "tar-fs": "^2.0.0",
    "uuidv4": "^4.0.0",
    "xml2js": "^0.4.19"
  },
  "engines": {
    "node": "10.15.3"
  }
}
